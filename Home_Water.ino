    display.drawBitmap(70, 0, water2, 50, 30, 1);
    display.display();
    delay(250);
    countTime += 1;
    Serial.print("Count Time = ");
    Serial.println(countTime);
  } else {
    if (BuzzerAlarm < 1) {
      count();
    } else {
      alarm();
    }
    delay(20);
  }

  if (countTime > 600) {  //ระยะการใช้งาน

    LINE.notify("Forget Switch");
    countTime -= 40;
  }
}
